{
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react",
    "target": "es2022",
    "module": "commonjs",
    "lib": ["dom", "es6", "es2017", "esnext.asynciterable"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "removeComments": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "paths": {
      "@slibs/common": ["libs/core/common/src"],
      "@slibs/common/*": ["libs/core/common/src/*"],
      "@slibs/api": ["libs/core/api/src"],
      "@slibs/api/*": ["libs/core/api/src/*"],
      "@slibs/testing": ["libs/core/testing/src"],
      "@slibs/testing/*": ["libs/core/testing/src/*"],
      "@slibs/database": ["libs/system/database/src"],
      "@slibs/database/*": ["libs/system/database/src/*"],
      "@slibs/storage": ["libs/system/storage/src"],
      "@slibs/storage/*": ["libs/system/storage/src/*"],
      "@slibs/redis-queue": ["libs/system/redis-queue/src"],
      "@slibs/redis-queue/*": ["libs/system/redis-queue/src/*"],
      "@slibs/imaging": ["libs/system/imaging/src"],
      "@slibs/imaging/*": ["libs/system/imaging/src/*"],
      "@slibs/comfy": ["libs/external/comfy/src"],
      "@slibs/comfy/*": ["libs/external/comfy/src/*"],
      "@slibs/app-shared": ["libs/app-shared/src"],
      "@slibs/app-shared/*": ["libs/app-shared/src/*"],
      "@slibs/user": ["libs/biz/user/src"],
      "@slibs/user/*": ["libs/biz/user/src/*"],
      "@slibs/admin": ["libs/biz/admin/src"],
      "@slibs/admin/*": ["libs/biz/admin/src/*"],
      "@slibs/mylab": ["libs/biz/mylab/src"],
      "@slibs/mylab/*": ["libs/biz/mylab/src/*"],
      "@slibs/ai-image": ["libs/biz/ai-image/src"],
      "@slibs/ai-image/*": ["libs/biz/ai-image/src/*"]
    }
  }
}
