{
  "$schema": "https://json.schemastore.org/package",
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "start:dev": "nest start --watch",
    "build": "nest build",
    "test": "ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "ENV=test jest --watch --runInBand",
    "typeorm": "ts-node -r dotenv/config -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:new": "pnpm typeorm migration:create migrations/migration",
    "migration:gen": "pnpm typeorm migration:generate migrations/migration -p -d ormconfig.ts",
    "migration:run": "pnpm typeorm migration:run -d",
    "migration:revert": "pnpm typeorm migration:revert -d",
    "db": "docker-compose -f res/docker-compose.yml up --build"
  },
  "dependencies": {
    "@bull-board/api": "6.5.3",
    "@bull-board/express": "6.5.3",
    "@bull-board/nestjs": "6.5.3",
    "@nestjs/bullmq": "10.2.2",
    "@nestjs/common": "10.4.6",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.6",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.6",
    "@nestjs/swagger": "8.0.0",
    "@nestjs/typeorm": "10.0.2",
    "@types/express-session": "1.18.0",
    "axios": "1.7.7",
    "bcrypt": "5.1.1",
    "bullmq": "5.22.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "connect-redis": "7.1.1",
    "dayjs": "1.11.13",
    "ejs": "3.1.10",
    "env-var": "7.5.0",
    "express": "4.21.1",
    "express-session": "1.18.1",
    "file-type": "16.5.4",
    "fs-extra": "11.2.0",
    "ioredis": "5.4.1",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "minio": "8.0.2",
    "nestjs-form-data": "1.9.91",
    "node-ssh": "12.0.5",
    "openai": "4.76.2",
    "passport": "0.7.0",
    "passport-custom": "1.1.1",
    "pg": "8.13.1",
    "ping": "0.4.4",
    "request-ip": "3.3.0",
    "rxjs": "7.8.1",
    "supertest": "7.0.0",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "4.1.0",
    "typeorm-transactional": "0.5.0",
    "uuid": "11.0.2",
    "ws": "8.18.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.5",
    "@nestjs/testing": "10.4.6",
    "@types/bcrypt": "5.0.2",
    "@types/express": "5.0.0",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.7",
    "@types/lodash": "4.17.13",
    "@types/node": "22.8.2",
    "@types/pg": "8.11.10",
    "@types/ping": "0.4.4",
    "@types/request-ip": "0.0.41",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.5.13",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "testcontainers": "10.13.2",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".+(e2e-spec|spec).ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@slibs/common(|/.*)$": "<rootDir>/libs/core/common/src/$1",
      "^@slibs/api(|/.*)$": "<rootDir>/libs/core/api/src/$1",
      "^@slibs/testing(|/.*)$": "<rootDir>/libs/core/testing/src/$1",
      "^@slibs/database(|/.*)$": "<rootDir>/libs/system/database/src/$1",
      "^@slibs/redis-queue(|/.*)$": "<rootDir>/libs/system/redis-queue/src/$1",
      "^@slibs/redis(|/.*)$": "<rootDir>/libs/system/redis/src/$1",
      "^@slibs/storage(|/.*)$": "<rootDir>/libs/system/storage/src/$1",
      "^@slibs/user(|/.*)$": "<rootDir>/libs/biz/user/src/$1",
      "^@slibs/infra-instance(|/.*)$": "<rootDir>/libs/biz/infra-instance/src/$1",
      "^@slibs/openai(|/.*)$": "<rootDir>/libs/external/openai/src/$1"
    }
  }
}
